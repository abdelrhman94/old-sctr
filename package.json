{
  "name": "sctr-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "build:dev": "env-cmd -f .env.development next build",
    "build:prod": "env-cmd -f .env.production next build",
    "build:stg": "env-cmd -f .env.staging next build",
    "build:qa": "env-cmd -f .env.testing next build",
    "build:all": "concurrently \"pnpm run build:dev\" \"pnpm run build:prod\" \"pnpm run build:stg\" \"pnpm run build:qa\"",
    "format": "prettier --write ."
  },
  "dependencies": {
